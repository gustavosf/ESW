<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.helloworld" version="1.0.0">
	<name>Brasileiro</name>
	<description>Classificação e jogos do Campeonato Brasileiro 2016</description>
	<author email="gustavosf@gmail.com" href="http://gustavo.seganfredo.ml"> Gustavo Seganfredo Farias</author>
	<content src="index.html"/>
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*"></access>
	<allow-navigation href="*" />
	<preference name="Orientation" value="portrait"/>
	<icon src="icon.png" platform="android" density="mdpi"></icon>
	<icon src="icon.png" platform="android" density="ldpi"></icon>
	<icon src="icon.png" platform="android" density="hdpi"></icon>
	<icon src="icon.png" platform="android" density="xhdpi"></icon>
</widget>